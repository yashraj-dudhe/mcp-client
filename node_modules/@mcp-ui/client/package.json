{
  "name": "@mcp-ui/client",
  "version": "2.5.1",
  "description": "mcp-ui Client SDK",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "*",
    "@quilted/threads": "^3.1.3",
    "@remote-dom/core": "^1.8.0",
    "@remote-dom/react": "^1.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "esbuild": "^0.25.5",
    "jsdom": "^22.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "pnpm run bundle:iframe && tsc && vite build",
    "bundle:iframe": "node scripts/bundle-iframe.js",
    "dev": "pnpm run bundle:iframe && vite",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "Apache-2.0"
}
